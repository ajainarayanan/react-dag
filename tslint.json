{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "arrow-parens": false,
    "object-literal-key-quotes": [
      false
    ],
    "member-ordering": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "ordered-imports": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  },
  "rulesDirectory": []
}
