    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-sprite-loader": "^3.4.1",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.7"
